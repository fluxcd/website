{{- $index := slice -}}
{{- range $resources := $.Site.Data.resources.resources -}}
    {{- $index = $index | append (dict "title" $resources.title "youtube" $resources.youtube "url" $resources.url "type" $resources.type "year" (time.Format "2006" $resources.date) "date" (time.Format "2 jan, 2006" $resources.date) ) -}}
{{- end -}}
{{- $index | jsonify -}}